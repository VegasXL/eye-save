<Application x:Class="EyeSave.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EyeSave"
             StartupUri="Views/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <SolidColorBrush x:Key="ColorPrimary">#FFF</SolidColorBrush>
            <SolidColorBrush x:Key="ColorSecondary">#FFE9F9</SolidColorBrush>
            <SolidColorBrush x:Key="ColorAccent">#B175EE</SolidColorBrush>
        </ResourceDictionary>
    </Application.Resources>
</Application>
